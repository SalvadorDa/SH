    TrWords:
    Setup:
    BTConnect:

<BTConnect>:
    name: "BTConnect"
    BoxLayout:
        orientation: "vertical"
        padding: 30
        spacing: 20
        Button:
            font_size: dp(20)
            font_name: 'fonts/bauhaus.ttf'
            text: "Connect"
            size_hint: 1, .2
            pos_hint: {"center_x": .5}
            on_press: root.connect_bt()
        Button:
            font_size: dp(20)
            font_name: 'fonts/bauhaus.ttf'
            text: "Connect 2"
            size_hint: 1, .2
            pos_hint: {"center_x": .5}
            on_press: root.con_bt()
        Button:
            font_size: dp(20)
            font_name: 'fonts/bauhaus.ttf'
            text: "Send A"
            size_hint: 1, .2
            pos_hint: {"center_x": .5}
            on_press: root.on()
        Button:
            font_size: dp(20)
            font_name: 'fonts/bauhaus.ttf'
            text: "Disconnect"
            size_hint: 1, .2
            pos_hint: {"center_x": .5}
            on_press: root.disconnect()
<MainWindow>:
    name: "MainWindow"
    BoxLayout:
        orientation: "vertical"
        padding: 30
        spacing: 20

        Button:
            font_size: dp(20)
            font_name: 'fonts/proto.ttf'
            text: "BT Connect"
            size_hint: .8, .25
            pos_hint: {"center_x": .5}
            # on_press: root.on_connect_click()
            # background_normal: ''
            background_color: 0, 0, .4, .95
            on_release:
                app.root.current = "BTConnect"
                root.manager.transition.direction = "up"
        Label:
            font_size: dp(40)
            font_name: 'fonts/20808.otf'
            text: "Sense Hand"
            size_hint: .1, .1
            pos_hint: {"center_x": .5, "top": 0}
        Button:
            font_size: dp(20)
            font_name: 'fonts/proto.ttf'
            text: "Start"
            size_hint: .8, .25
            pos_hint: {"center_x": .5}
            # on_press: root.on_button1_click()
            # background_normal: ''
            background_color: 0, 0, .3, .95
        Button:
            font_size: dp(20)
            font_name: 'fonts/proto.ttf'
            text: "Training"
            size_hint: .8, .25
            pos_hint: {"center_x": .5}
            # on_press: root.on_button2_click()
            # background_normal: ''
            background_color: 0, 0, .2, .95
            on_release:
                app.root.current = "TrainingWindow"
                root.manager.transition.direction = "left"
        Button:
            font_size: dp(20)
            font_name: 'fonts/proto.ttf'
            text: "Setup"
            size_hint: .8, .25
            pos_hint: {"center_x": .5}
            # on_press: root.on_button3_click()
            # background_normal: ''
            background_color: 0, 0, 0.1, .95
            on_release:
                app.root.current = "Setup"
                root.manager.transition.direction = "left"

